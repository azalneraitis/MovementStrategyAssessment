<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MovementStrategyAssessment" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/orange_600</item>
        <item name="colorPrimaryVariant">@color/orange_800</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/blue_300</item>
        <item name="colorSecondaryVariant">@color/blue_800</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="PositiveButtonStyle" parent="TextAppearance.MaterialComponents.Button">
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@color/blue_800</item>
    </style>
    <style name="NegativeButtonStyle" parent="TextAppearance.MaterialComponents.Button">
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@color/blue_300</item>
    </style>
</resources>